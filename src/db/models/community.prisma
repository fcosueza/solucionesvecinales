model Community {
  id       Int    @id @default(autoincrement())
  name     String
  street   String
  number   Int
  city     String
  province String
  country  String

  tenants   User[]     @relation(name: "tenant")
  messages  Message[]
  areas     Area[]
  incidents Incident[]
  requests  Request[]

  adminId String
  admin   User   @relation("admin", fields: [adminId], references: [id])
}
