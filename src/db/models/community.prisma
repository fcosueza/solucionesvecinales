model Community {
  id       Int    @id @default(autoincrement())
  name     String
  street   String
  number   Int
  city     String
  province String
  country  String

  messages  Message[]
  areas     Area[]
  incidents Incident[]
  requests  Request[]

  tenants User[] @relation(name: "tenant")
  adminID String @unique
  admin   User   @relation("admin", fields: [adminID], references: [id])
}
